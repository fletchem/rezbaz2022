@article{ WOS:000433675100003,
Author = {Bahn, Sung-sik and Seo, Sang-gu and Park, Jong-hae and Jang, Sung-hee},
Title = {Entrepreneurial Attitudes, Activities, and Aspirations as Driving Forces
   of Entrepreneurial Ecosystem in Korea},
Journal = {ASIA PACIFIC JOURNAL OF INNOVATION AND ENTREPRENEURSHIP},

@article{ WOS:000765695800016,
Author = {Zhao, Jianhang and Xu, Zhaoxu and Liu, Yan and Wang, Xiaobin and Liu,
   Xinli and Gao, Yanan and Jin, Ying},
Title = {The expression of cancer-testis antigen in ovarian cancer and the
   development of immunotherapy},
Journal = {AMERICAN JOURNAL OF CANCER RESEARCH},
Year = {2022},
Volume = {12},
Number = {2},
Pages = {681-694},
Abstract = {Ovarian cancer is a relatively common tumor in women with the highest
   mortality among female reproductive system tumors. The lack of apparent
   early symptoms and effective screening strategies often leads to ovarian
   cancer being diagnosed at an advanced stage. Immunotherapy relying on
   tumor-associated antigens might improve the treatment of ovarian cancer.
   Cancer-testis antigens (CTAs) are ideal tumor-associated antigens, and
   MAGE-A, NY-ESO-1, CT45, and Sp17 are classic CTAs highly expressed in

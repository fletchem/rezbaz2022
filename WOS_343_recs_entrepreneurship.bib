@article{ WOS:000777465700004,
Author = {Huaman, Fatima and Guede, Belen and Cancino, Christian A. and Cordova,
   Miguel},
Title = {Female entrepreneurship: Evidence-based high-impact perspective from
   Chile and Peru},
Journal = {ESTUDIOS GERENCIALES},
Year = {2022},
Volume = {38},
Number = {162},
Pages = {45-56},
Month = {JAN-MAR},
Abstract = {This study aims to highlight cases of women-led business (WLB) that
   achieved high-growth in the short term, contradicting the traditional
   approach of female entrepreneurship associated with precariousness. This
   paper uses a multiple case study, exploring examples of WLBs in Chile

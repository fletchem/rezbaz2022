@incollection{ WOS:000276911900006,
Author = {Lofstrom, Magnus and Bates, Timothy},
Editor = {Butler, JS and Morales, A and Torres, DL},
Title = {How Successful are Female Hispanic Entrepreneurs?},
Booktitle = {AMERICAN STORY: MEXICAN AMERICAN ENTREPRENEURSHIP AND WEALTH CREATION},
Series = {International Series on Technology Policy and Innovation},
Year = {2009},
Pages = {79-97},
Abstract = {This paper analyzes the relative success of self-employed female
   Hispanics. using individual panel data from the 1996 and 2001 Survey of
   income and program participation (SIPP), we generate different earnings
   measures, allowing a comparison of earnings between self-employed and
   wage or salary employed women. We compare earnings of Hispanic female
   entrepreneurs to both Latina wage or salary workers and to female
   non-Hispanic white wage or salary employees. Our findings indicate that
   Latina entrepreneurs do quite well, once differences in mean observable
   characteristics, such as education, are taken into account.
   Self-employed Latinas are estimated to earn more than observationally
   similar nonminority white female entrepreneurs and roughly the same as
   observationally similar Latinas in wage or salary work.},

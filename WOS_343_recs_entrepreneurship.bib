Title = {TERRITORIAL INTENSIVE PRODUCTS AS PROMOTERS OF REGIONAL TOURISM. THE
   CASE STUDY OF DOURO SKINCARE},
Journal = {HOLOS},
Year = {2018},
Volume = {34},
Number = {4},
Pages = {122-135},
Abstract = {The case study explores the growth and regional involvement of Douro
   SkinCare(1), a company created by women that operates in the field of
   selective Biological Cosmetics through the creation, development and
   production of cosmetic lines that are based on emblematic raw materials

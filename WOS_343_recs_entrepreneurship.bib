@inproceedings{ WOS:000380498100027,
Author = {Akor, Faith},
Editor = {Ramjugernath, D},
Title = {Women Entrepreneurship Development in Benue State: A Comparative Study
   of Tiv and Idoma Women, 1960-2012},
Booktitle = {PROCEEDINGS OF THE 3RD INTERNATIONAL CONFERENCE ON INNOVATION AND
   ENTREPRENEURSHIP (ICIE-2015)},
Year = {2015},

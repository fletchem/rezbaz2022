@article{ WOS:000430988900034,
Author = {Bian Xunguang and Li Jiancheng and Xu Chu and Yang Li},
Title = {Immunolocalization and Expression of Androgen Receptor in the Oviduct of
   Laying Hens},
Journal = {PAKISTAN JOURNAL OF ZOOLOGY},

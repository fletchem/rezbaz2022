@inproceedings{ WOS:000310338800078,
Author = {Moghavvemi, Sedigheh and Salleh, Noor A. Mohd and Zhao, Wenjie and Kang,
   Xianxue},
Editor = {Fulford, H},
Title = {A Study of IT Innovation Adoption and Entrepreneurship in Malaysia},

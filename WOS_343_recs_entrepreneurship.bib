@article{ WOS:000653124000001,
Author = {Chavez Rivera, Maria Eulalia and Fuentes Fuentes, Maria del Mar and
   Maria Ruiz-Jimenez, Jenny},
Title = {Challenging the context: mumpreneurship, copreneurship and sustainable
   thinking in the entrepreneurial process of women - a case study in
   Ecuador},
Journal = {ACADEMIA-REVISTA LATINOAMERICANA DE ADMINISTRACION},

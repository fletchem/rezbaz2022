@article{ WOS:000496810700006,
Author = {Woolley, Jennifer L.},
Title = {GENDER, EDUCATION, AND OCCUPATION: HOW FOUNDER EXPERIENCES INFLUENCE
   FIRM OUTCOMES},
Journal = {ACADEMY OF MANAGEMENT DISCOVERIES},

@article{ WOS:000410013400003,
Author = {Strohmeyer, Robert and Tonoyan, Vartuhi and Jennings, Jennifer E.},
Title = {Jacks-(and Jills)-of-all-trades: On whether, how and why gender
   influences firm innovativeness},
Journal = {JOURNAL OF BUSINESS VENTURING},
Year = {2017},
Volume = {32},
Number = {5},
Pages = {498-518},
Month = {SEP},
Abstract = {This study examines whether, how and why an entrepreneur's gender
   influences firm innovativeness. Drawing upon research on
   gender-segregated educational and employment experiences, we suggest
   that firms led by women will exhibit less innovation breadth and depth
   than those led by men due to differences in the degree to which male
   versus female entrepreneurs resemble jacks-(or jills)-of-all trades'.
   Analysis of survey data collected from 900 business owner-managers in
   Germany lends overall support albeit with some noteworthy exceptions.
   Our findings point to the need for an even more nuanced approach to
   research at the nexus of gender, entrepreneurship, and innovation. (C)
   2017 Elsevier Inc. All rights reserved.},

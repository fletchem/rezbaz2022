@article{ WOS:000528313800013,
Author = {Mendonca, Joana and Reis, Anabela},
Title = {Exploring the mechanisms of gender effects in user innovation},
Journal = {TECHNOLOGICAL FORECASTING AND SOCIAL CHANGE},
Year = {2020},
Volume = {155},
Month = {JUN},
Abstract = {Entrepreneurship research has shown significant differences between
   genders on what concerns technology commercialization. However, most
   evidence on gender differences focuses on producer centred processes,
   which is not applicable to user innovation. Furthermore, there is no
   rigorous evidence explaining these differences in user innovation
   between male and female individuals. This paper contributes to shed some
   light on why these differences exist for user innovation, and proposes a
   framework to explain the role of gender in user innovation, considering
   both a direct and an indirect effect. We look at the users' probability
   to innovate by gender, investigating direct and indirect effects through
   fields of education and personality traits, such as risk taking,
   innovativeness and aspirations. Using data from a unique survey on user
   innovation, we find that although men innovate more, female show no
   clear differences on innovation behaviour and characteristics of
   innovations. We also find that gender has a direct effect on the
   probability of users to innovate and an indirect effect on user
   innovation through education fields, but that the direct effect of
   gender on innovation is stronger. Our results providence supported for
   our proposed framework.},
